{
    "apiVersion": "extensions/v1beta1",
    "kind": "Ingress",
    "metadata": {
        "annotations": {
            "ingress.kubernetes.io/proxy-body-size": "100m",
            "kubernetes.io/ingress.class": "nginx"
        },
        "name": "$name"
    },
    "spec": {
        "rules": [
            {
                "host": "addressextractor.demo.sprinthive.tech",
                "http": {
                    "paths": [
                        {
                            "backend": {
                                "serviceName": "$name",
                                "servicePort": 80
                            },
                            "path": "/"
                        }
                    ]
                }
            }
        ]
    }
}
